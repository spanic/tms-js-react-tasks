{
  "extends": ["react-app", "react-app/jest", "prettier"],
  "rules": {
    "indent": [
        "error",
        2
    ],
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true
      }
    ],
    "no-restricted-globals": [
      "off",
      {
          "name": "confirm"
      }
    ]
  }
}
